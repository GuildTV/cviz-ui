{
  "name": "cviz-ui",
  "private": true,
  "version": "0.3.0",
  "description": "A simple web interface for cviz",
  "scripts": {
    "start": "cross-env NODE_ENV=production ./node_modules/.bin/babel-node ./server.js",
    "dev": "cross-env NODE_ENV=development nodemon --ignore dumps/ --ignore js/ --ignore state.json --exec babel-node ./server.js",
    "lint": "cross-env NODE_ENV=test eslint --ext .js .",
    "migrate": "cross-env NODE_ENV=development sequelize --config=sequelize.json db:migrate",
    "gulp": "gulp"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/GuildTV/cviz-ui.git"
  },
  "dependencies": {
    "JSONStream": "^1.3.1",
    "axios": "^0.15.2",
    "babel-cli": "^6.5.1",
    "babel-core": "^6.0.12",
    "babel-loader": "^6.0.1",
    "babel-preset-es2015": "^6.0.12",
    "babel-preset-react": "^6.0.15",
    "babel-preset-stage-0": "^6.5.0",
    "body-parser": "^1.13.3",
    "bootstrap": "^3.3.6",
    "classnames": "^2.1.3",
    "cross-env": "^5.1.1",
    "express": "^4.13.1",
    "gulp": "^3.9.1",
    "gulp-util": "^3.0.7",
    "history": "^1.15.0",
    "mysql": "^2.12.0",
    "nunjucks": "^3.0.0",
    "promise-map-series": "^0.2.2",
